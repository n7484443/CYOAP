<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ContextMenu?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cyoap_main.design.controller.MakeGUIController">
   <children>
      <SplitPane fx:id="pane_mainGui" dividerPositions="0.5" prefHeight="540.0" prefWidth="960.0" snapToPixel="false">
        <items>
          <AnchorPane maxHeight="538.0" maxWidth="169.0" minHeight="538.0" minWidth="169.0" prefHeight="538.0" prefWidth="169.0" SplitPane.resizableWithParent="false">
               <children>
                  <TextField layoutX="4.0" layoutY="3.0" />
                  <ListView fx:id="var_field" layoutX="4.0" layoutY="32.0" prefHeight="406.0" prefWidth="161.0" />
                  <ListView fx:id="var_type" layoutX="4.0" layoutY="441.0" prefHeight="95.0" prefWidth="161.0" />
               </children>
            </AnchorPane>
            <TabPane fx:id="tabpane_make" prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE">
              <tabs>
                  <Tab fx:id="tab_position" text="Position">
                    <content>
                      <AnchorPane fx:id="pane_position" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                    </content>
                     <contextMenu>
                        <ContextMenu fx:id="menu_mouse">
                          <items>
                            <MenuItem fx:id="menu_create" mnemonicParsing="false" text="Create" />
                            <MenuItem fx:id="menu_delete" mnemonicParsing="false" text="Delete" />
                          </items>
                        </ContextMenu>
                     </contextMenu>
                  </Tab>
                <Tab fx:id="tab_describe" text="Describe">
                     <content>
                      <AnchorPane fx:id="pane_describe" minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
                           <children>
                              <Button fx:id="button_next" layoutX="736.0" layoutY="484.0" mnemonicParsing="false" text="Next" />
                              <TextArea fx:id="text_info" layoutX="6.0" layoutY="262.0" prefHeight="217.0" prefWidth="771.0" />
                              <RadioButton layoutX="6.0" layoutY="487.0" mnemonicParsing="false" text="DarkMode" />
                              <Button fx:id="button_save" layoutX="690.0" layoutY="484.0" mnemonicParsing="false" text="Save" />
                              <TextField fx:id="text_title" alignment="CENTER" layoutX="6.0" layoutY="3.0" prefHeight="22.0" prefWidth="771.0" text="Title" />
                              <HBox alignment="CENTER" layoutY="28.0" prefHeight="231.0" prefWidth="783.0">
                                 <children>
                                    <ImageView fx:id="imageView" fitHeight="227.0" fitWidth="779.0" pickOnBounds="true" preserveRatio="true" />
                                 </children>
                              </HBox>
                           </children>
                        </AnchorPane>
                     </content>
                </Tab>
              </tabs>
            </TabPane>
        </items>
      </SplitPane>
   </children>
</AnchorPane>
